<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">

    </style>
    <style name="WindowTheme" parent="AppTheme">
        <item name="android:background">@color/dn_white</item>
    </style>

    <style name="Label" parent="AppTheme">
        <item name="android:textStyle">bold</item>
    </style>

    <style name="Divider" parent="AppTheme">
        <item name="android:background">@drawable/ic_launcher</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:layout_marginBottom">4dp</item>
        <item name="android:layout_marginTop">4dp</item>
    </style>


</resources>
